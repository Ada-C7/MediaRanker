<main class="row">

  <h3>List of Books</h3>
  <section class ="media-table">
    <table>
      <thread>
        <tr>
          <th>Votes<th>
            <th>Title</th>
            <th>Created By</th>
            <th>Pulbished</th>
            <th>Upvote</th>
          </tr>

        </tbody>
        <% @books.each do |book| %>
        <tr><td><%= link_to book.title, work_path(book) %>
          <small>by: <%= book.creator %><small>
            <p># Votes</p>
          </td></tr>
          <% end %>
        </tbody>
      </table>
    </section>
  </row>



