<h4>Something</h4>
<table>
  <tr>
    <th>Votes:</th>
    <th>Title:</th>
    <th>Created By:</th>
    <th>Published:</th>
    <th>Upvotes:</th>
  </tr>
  <% @works.each do |work| %>
    <tr>
      <td> Votes </td>
      <td><%= link_to "#{work.title}", work_path(work.id) %></td>
      <td><%= "#{work.creator}" %></td>
      <td><%= "#{work.publication_year}" %></td>
      <td> Upvotes Button </td>
    </tr>
  <% end %>
</table>
