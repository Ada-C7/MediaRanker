<h1>FORM!!!!!!</h1>
<%= form_for @book, {method: form_method, url: '/books'} do |f| %>
    <%= f.label :heheh %>
    <%= f.text_field :title %>

    <%= f.label :creator%>
    <%= f.text_field :creator %>

    <%= f.label :publication_year %>
    <%= f.text_field :publication_year %>

    <%= f.label :description %>
    <%= f.text_field :description %>

    <%= f.submit action_name %>
<% end %>
