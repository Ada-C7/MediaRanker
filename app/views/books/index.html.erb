<h3>List of books</h3>

<section>
  <table>
    <thead>
      <tr>
        <th>Votes</th>
        <th>Title</th>
        <th>Created by</th>
        <th>Published</th>
        <th>Upvote</th>
      </tr>
    </thead>
    <tbody>
      <% @books.each do |book| %>
      <tr>
        <td></td>
        <td><%= book.title %></td>
        <td><%= book.creator %></td>
        <td><%= book.pub_year %></td>
        <td></td>
      </tr>
      <% end %>
    </tbody>



  </table>
</section>
