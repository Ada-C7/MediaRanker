<h1>Users#index</h1>
<table>
  <tr>
    <th>Username:</th>
    <th>Votes:</th>
    <th>Joined:</th>
  </tr>
  <% @users.each do |user| %>
    <tr>
      <td><%= link_to "#{user.username}", user_path(user.id) %></td>
      <td><%= user.votes.count %></td>
      <td><%= user.created_at %></td>
    </tr>
  <% end %>
</table>
