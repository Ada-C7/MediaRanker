<h1>List of Users</h1>
<section>
<table>
  <thead>
    <tr>
      <th>Username</th>
      <th>Votes</th>
      <th>Joined </th>
    </tr>
  </thead>
  <tbody>
  <% @users.each do |user| %>
    <tr>
        <td><%= link_to user.name, user_path(user.id) %></td>
        <td> <%=user.votes.count %></td>
        <td> <%= user.created_at %></td>
    </tr>
  <% end %>
    </tbody>
</table>
</section>

<p><%= link_to "View all Media", welcome_path, class: "button" %></p>
