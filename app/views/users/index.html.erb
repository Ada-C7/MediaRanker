<body>
  <h3>List of Users</h3>

  <table>
    <tr>
      <th>Username</th>
      <th>Votes</th>
      <th>Joined</th>
    </tr>
    <% @users.each do |user| %>
    <tr>
      <td><%= link_to user.username, user_path(user.id) %></td>
      <td><%= user.votes.count %></td>
      <td><%= user.created_at.strftime("%b %d,   %Y") %></td>
    </tr>
    <% end %>
  </table>
  <div class = "button"><%= link_to "Back to Media List", main_path %></div>
</body>
