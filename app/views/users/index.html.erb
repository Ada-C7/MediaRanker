<h2>List of Users</h2>

<table>
  <tr>
    <th>Username</th>
    <th>Votes</th>
    <th>Joined</th>
  </tr>
  <% @users.each do |user|%>
  <tr>
    <td><%= link_to user.username, user_path(user.id)%></td>
    <td> vote count </td>
    <td><%= work.created_at %></td>
  </tr>
