<h3>List of Users</h3>

<p><%= "Joined site #{current_user.created_at.strftime("%B %d, %Y")}" %></p>

<h4>Votes</h4>
<table>
  <thead>
    <tr>
      <th>Username</th>
      <th>Votes</th>
      <th>Joined</th>
    </tr>
  </thead>
  <tbody>
    <% User.all.each do |user|%>
    <tr>
      <td> <%=link_to user.username, user_path(user), method: :get%> </td>
    <td> VOTES          </td>
      <td> <%= user.created_at.strftime("%B %d, %Y")%> </td>
    </tr>
    <% end %>
  </tbody>
</table>

<%= button_to "Back to Media List", items_path, method: :get%>
