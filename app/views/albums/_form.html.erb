<%= form_for @album, url: album_path do |f| %>

  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :creator  %>
  <%= f.text_field :creator %>

  <%= f.label :publication_year %>
  <%= f.text_field :publication_year %>

  <%= f.label :description %>
  <%= f.text_area :description, size: "60x12" %>

  <%= f.submit "Submit" %>
<% end %>
