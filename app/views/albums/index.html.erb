<h2>List of Albums</h2>
<table>
  <tr>
    <th>Votes</th>
    <th>Title</th>
    <th>Created by</th>
    <th>Published</th>
    <th>Upvote</th>
  </tr>
  <% @albums.each do | album | %>
  <tr>
    <td>(number of votes)</td>
    <td><%= link_to album.title, work_path(album.id) %></td>
    <td><%= album.creator %></td>
    <td><%= album.year %></td>
    <td>(upvote button)</td>
  </tr>
</table>
