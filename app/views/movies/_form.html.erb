<h1>MOVIE FORM!!!!!!</h1>
<%= form_for @movie, {method: form_method, url: '/movies'} do |f| %>
    <%= f.label :title%>
    <%= f.text_field :title %>

    <%= f.label :creator%>
    <%= f.text_field :creator %>

    <%= f.label :publication_year %>
    <%= f.text_field :publication_year %>

    <%= f.label :description %>
    <%= f.text_field :description %>

    <%= f.submit action_name %>
<% end %>
